package com.capg.day8;

public class MyEmployee {
	 public static final int getsal = 0;

	@Override
	public String toString() {
		return "MyEmployee [empid=" + empid + ", ename=" + ename + ", sal=" + sal + "]";
		
		public MyEmployee(int empid, String name, int sal) {
			 this.empid=empid;
			 this.ename=name;
			 this.sal=sal;
	}
	int empid;
	 String ename;
	 double sal;
	 
	public int getEmpid() {
		return empid;
	}
	public void setEmpid(int empid) {
		this.empid = empid;
	}
	public String getEname() {
		return ename;
	}
	public void setEname(String ename) {
		this.ename = ename;
	}
	public double getSal() {
		return sal;
	}
	public void setSal(double sal) {
		this.sal = sal;
	}

}
