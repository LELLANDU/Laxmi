package com.capg.day8;

public class MyEmployee {
	 
	@Override
	public String toString() {
		return "MyEmployee [empid=" + empid + ", ename=" + ename + ", salary=" + salary + "]";
		
		public MyEmployee(int empid, String name, int salary) {
			 this.empid=empid;
			 this.ename=name;
			 this.salary=salary;
	}
	int empid;
	 String ename;
	 double salary;
	 
	public int getEmpid() {
		return empid;
	}
	public void setEmpid(int empid) {
		this.empid = empid;
	}
	public String getEname() {
		return ename;
	}
	public void setEname(String ename) {
		this.ename = ename;
	}
	public double getSalary() {
		return salary;
	}
	public void setSal(double salary) {
		this.salary = salary;
	}

}
