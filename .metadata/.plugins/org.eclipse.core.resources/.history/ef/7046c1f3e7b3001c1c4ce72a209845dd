package com.capg.day6;
import java.util.*;

public class ArrayList {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
       ArrayList<Integer> ls=new ArrayList<Integer>();
       ls.add(15);
       ls.add(20);
       ls.add(10);
       ls.add(100);
       System.out.println(ls.contains(101));
	}

}
