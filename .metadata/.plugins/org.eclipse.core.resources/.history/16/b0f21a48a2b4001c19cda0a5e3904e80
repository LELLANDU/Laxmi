package com.capg.day8;

interface EvenOdd{
	void check(int a) ;
		
	}

public class MyLambda1 {

	public static void main(String[] args) {
		EvenOdd evenOdd=(int a)->{
			if(a%2==0) {
				System.out.println("number "+a+"is even");
			}
			else
				System.out.println("number "+a+"is odd");
			};
			
		
		
		evenOdd.check(11);
		}
	    	
	}
